AutofarmSection:NewToggle("Auto Swing", "Swing for you", function(state)
    if state then
        getgenv().katanaswinger = true
            while wait(0.1) do
                if getgenv().katanaswinger == true then
                    local A_1 = "swingKatana"
                        local Event = game:GetService'Players'.LocalPlayer.ninjaEvent
                    Event:FireServer(A_1)
                end
        end
    else
        getgenv().katanaswinger = false
    end
end)
Autofarm3Section:NewToggle("Auto Sell", "Automatically Sells Ninjitsu for you", function(state)
    if state then
getgenv().sell = true
while wait() do
    if getgenv().sell == true then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(77.4037704, 3.22484112, -49.3583145)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(136.094116, 28259.8945, 62.6110229)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(77.4037704, 3.22484112, -49.3583145)
wait(0.1)
end
end
    else
        getgenv().sell = false
    end
end)
