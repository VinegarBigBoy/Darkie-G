Autofarm2Section:NewToggle("Auto Buy Swords", "Does what you think it does", function(state)
    if state then
        getgenv().buyAllSwords = true
            while wait(1) do
              if getgenv().buyAllSwords == true then
                 local swordstring1 = "buyAllSwords"
                    local swordstring2 = "Ground"
                game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(niggastring1, niggastring2)
              end
        end
    else
        getgenv().buyAllSwords = false
    end
end)
Autofarm2Section:NewToggle("Auto Buy Belts", "Does what you think it does", function(state)
    if state then
        getgenv().buybelt = true
            while wait(1) do
              if getgenv().buybelt == true then
                local beltstring1 = "buyAllBelts"
                    local beltstring2 = "Ground"
                game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(beltstring1, beltstring2)
              end
        end
    else
        getgenv().buybelt = false
    end
end)
Autofarm2Section:NewToggle("Auto Buy Skills", "Does what you think it does", function(state)
    if state then
        getgenv().buyskill = true
            while wait(1) do
              if getgenv().buyskill == true then
                local skillstring = "buyAllSkills"
                    local skillstring = "Ground"
                game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(ohString1, ohString2)
              end
         end
    else
        getgenv().buyskill = false
    end
end)
