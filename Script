SettingsSection:NewKeybind("Set the GUI toggle", "You Should Know", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
BuyRegUpgradesSection:NewToggle("Auto Purchase Click Multiply", "Purchases Click Multipliers perk", function(state)
    if state then  
        getgenv().clickmult = true
        while wait() do
            if getgenv().clickmult == true then
                local A_1 = "ClickMultiply"
                    local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.UpgradeService.BuyUpgrade
                        Event:FireServer(A_1)
                end
        end
    else
        getgenv().clickmult = false
    end
end)
