TpSection:NewToggle("Flag AutoFarm", "Teleports player to a flag every 7 seconds", function(state)
    if state then
        getgenv().autoflags = true
        while wait(0.3) do
            if getgenv().autoflags == true then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(428.125732, 23.7667828, -424.628326)
                wait(7.3)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(655.642578, 23.7667408, -477.184814)
                wait(7.3)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(608.588196, 21.0696583, -233.459366)
                wait(7.3)
            end
        end
    else
        getgenv().autoflags = false
    end
end)
TpSection:NewToggle("KOTH AutoFarm", "Teleports player to KOTH", function(state)
    if state then
        getgenv().autok = true
        while wait(0.3) do
            if getgenv().autok == true then
        wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(546.53064, 32.5750542, -173.389252)
        wait()
            end
        end
    else
        getgenv().autok = false
    end
end)
ClickEditSection:NewButton("Redeem all codes", "Uses all codes", function()
local A_1 = "30KLIKES"
local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CodeService.Redeem
Event:InvokeServer(A_1)
wait()
local A_1 = "10KLIKES"
local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CodeService.Redeem
Event:InvokeServer(A_1)
wait()
local A_1 = "Release"
local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CodeService.Redeem
Event:InvokeServer(A_1)
wait()
local A_1 = "ELSA"
local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CodeService.Redeem
Event:InvokeServer(A_1)
wait()
local A_1 = "Test"
local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CodeService.Redeem
Event:InvokeServer(A_1)
end)
end
