if game.PlaceId == 5490351219 then
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Clicker Madness By Vinegar#7722", "BloodTheme")
--tabs
local AutoTab = Window:NewTab("Autofarms")
local RebirthTab = Window:NewTab("Rebirths")
local TeleportTab = Window:NewTab("Teleports")
local StateditTab = Window:NewTab("Stat editor")
local PurchaseTab = Window:NewTab("Purchase")
local SettingsTab = Window:NewTab("Settings")

--Sections
local AutoSection = AutoTab:NewSection("Basic Autofarms")
local AutoBossSection = AutoTab:NewSection("Boss Autofarm")
local TpSection = AutoTab:NewSection("Teleport Autofarms")
local TeleportSection = TeleportTab:NewSection("World Teleports")
local basicStateditSection = StateditTab:NewSection("Basic Stats")
local StateditSection = StateditTab:NewSection("Gold Gem Giver")
local ClickEditSection = StateditTab:NewSection("Infinite stuff")
local RebirthbuySection = RebirthTab:NewSection("Rebirth Buyer")
local BuyRegUpgradesSection = PurchaseTab:NewSection("Gem Upgrades")
local BuySpecialUpgradesSection = PurchaseTab:NewSection("Golden Gem Upgrades")
local BuyEggSection = PurchaseTab:NewSection("Buy 1 Egg")
local BuyEgg3Section = PurchaseTab:NewSection("Buy 3 Eggs")
local AutoBuyEggSection = PurchaseTab:NewSection("Auto Egg Buyer")
local autoRebirthbuySection = RebirthTab:NewSection("Auto Rebirth")
local SettingsSection = SettingsTab:NewSection("Settings")

--Functions
AutoSection:NewToggle("Clicker Autofarm", "Autofarm that is x10", function(state)
    if state then
        getgenv().basicfarmer = true
	while wait() do
		if getgenv().basicfarmer == true then
			game:service'ReplicatedStorage'.Aero.AeroRemoteServices.ClickService.Click:FireServer("100")
		end
	end
        print("Toggle On")
    else
        getgenv().basicfarmer = false
        print("Toggle Off")
    end
end)
AutoSection:NewToggle("Auto Beast Mode", "Automatically goes into beast mode UwU", function(state)
    if state then
        getgenv().autobeast = true
	while wait(2.5) do
		if getgenv().autobeast == true then
			local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.BeastModeService.Begin
            Event:FireServer()
		end
	end
        print("Toggle On")
    else
        getgenv().autobeast = false
        print("Toggle Off")
    end
end)
AutoBossSection:NewToggle("Auto Kill Boss", "Attacks boss at any distance", function(state)
    if state then
        getgenv().bossyfarmer = true
	while wait() do
		if getgenv().bossyfarmer == true then
			local A_1 = "Karen Keyboard"
                local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CursorCannonService.FireBoss
            Event:FireServer(A_1)
		end
	end
        print("Toggle On")
    else
        getgenv().bossyfarmer = false
        print("Toggle Off")
    end
end)
RebirthbuySection:NewButton("1 Rebirth", "Purchases 1 rebirth", function()
    local A_1 = 1
        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.RebirthService.BuyRebirths
            Event:FireServer(A_1)
            print("Purchased")
end)
