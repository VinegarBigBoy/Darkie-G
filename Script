	while wait() do
		if getgenv().Rebirth1t == true then
			    local A_1 = 100000000
                    local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.RebirthService.BuyRebirths
            Event:FireServer(A_1)
		end
	end
        print("Toggle On")
    else
        getgenv().Rebirth1t = false
        print("Toggle Off")
    end
end)
