local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.EggService.Purchase
Event:FireServer(A_1)
wait(0.0000000000001)
local A_1 = "infinity"
local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.EggService.Purchase
Event:FireServer(A_1)
wait(0.0000000000001)
local A_1 = "infinity"
local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.EggService.Purchase
Event:FireServer(A_1)
end)
