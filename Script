AutoBuyEggSection:NewToggle("Auto Fire Egg", "Auto Purchases a Fire egg", function(state)
    if state then
        getgenv().autofire = true
        while wait(0.3) do
            if getgenv().autofire == true then
                local A_1 = "fire"
                    local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.EggService.Purchase
                Event:FireServer(A_1)
            end
        end
    else
        getgenv().autofire = false --this part sucks lol
    end
end)
